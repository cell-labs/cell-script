package main

import (
	"cell"
	"debug"
	"tx"
)

function main() {
	args := tx.xudtArgs()
	err := tx.getErr(args)
	if err != 0 {
		return err
	}
	tx.checkEnhancedOwnerMode(args)
	err = tx.getErr(args)
	if err != 0 {
		return err
	}
	ret := tx.sudt(tx.getOwnerMode())
	if ret != 0 {
		return ret
	}
	if tx.getFlags(args) == tx.XUDTFlagsPlain {
		return 0
	}
	tx.executeScripts(args)
	return 0
}
