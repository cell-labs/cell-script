package bytes

import (
	// "internal/bytealg"
	"unicode"
	"unicode/utf8"
)

func Equal(a []byte, b []byte) bool {
	// Neither cmd/compile nor gccgo allocates for these string conversions.
	if len(a) != len(b) {
		return false
	}
	for i, vai := range a {
		if vai != b[i] {
			return false
		}
	}
	return true
}
